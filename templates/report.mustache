{{!
    @template local_coursereport/report
}}

<span class="py-3 float-right">
    <a href="{{config.wwwroot}}/local/coursereport/index.php" class="btn btn-light">
    {{#str}} back, local_coursereport {{/str}}</a>
</span>

<h3>{{name}}</h3>

<table class="table">
    <thead>
        <tr>
            <th scope="col">{{#str}} course, local_coursereport {{/str}}</th>
            <th scope="col">{{#str}} completion, local_coursereport {{/str}}</th>
            <th scope="col">{{#str}} timecomplete, local_coursereport {{/str}}</th>
        </tr>
    </thead>
    <tbody>
    {{#row}}
        <tr>
            <td>{{{course}}}</td>
            <td>{{completion}}</td>
            <td>{{timecomplete}}</td>
        </tr>
    {{/row}}
    </tbody>
</table>